class CustomClientContext(ClientContext):
    """
    è‡ªè¨‚çš„ ClientContextï¼Œè™•ç† SSL æ†‘è­‰å•é¡Œ
    å°±åƒè‡ªå·±æ”¹è£æ©Ÿè»Šä¸€æ¨£ï¼
    """
    def __init__(self, url):
        super().__init__(url)
        # è¦†å¯« requests çš„ session è¨­å®š
        self.pending_request().beforeExecute += self._patch_request
    
    def _patch_request(self, request):
        """ä¿®æ”¹æ¯å€‹ request çš„ SSL è¨­å®š"""
        # è¨­å®šä¸é©—è­‰ SSL
        request.verify = False
        return request

