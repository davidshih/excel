def quick_share_no_inheritance_break(site_url, folder_url, folder_name):
    """
    å¿«é€Ÿåˆ†äº«å‡½å¼ - ä¸ä¸­æ–·æ¬Šé™ç¹¼æ‰¿ç‰ˆæœ¬
    å°±åƒ Foodpanda ä¸€æ¨£å¿«é€Ÿï¼Œä½†ä¸æœƒæŠŠåŸæœ¬çš„é¤å»³é—œæ‰ï¼
    """
    print("ğŸš€ SharePoint è³‡æ–™å¤¾åˆ†äº«å·¥å…·")
    print("   ï¼ˆä¸ä¸­æ–·æ¬Šé™ç¹¼æ‰¿ç‰¹åˆ¥ç‰ˆï¼‰")
    print("=" * 50)
    
    # å»ºç«‹åˆ†äº«å·¥å…·
    sharer = SharePointAutoSharer(site_url)
    
    # é€£æ¥
    if not sharer.connect():
        return False
    
    # å¾ URL è§£æè·¯å¾‘
    parsed = urlparse(folder_url)
    base_path = parsed.path
    
    # ç¢ºä¿è·¯å¾‘æ ¼å¼æ­£ç¢º
    if not base_path.endswith('/'):
        base_path += '/'
    
    # å®Œæ•´è³‡æ–™å¤¾è·¯å¾‘
    full_folder_path = base_path + folder_name
    print(f"\nğŸ“ å®Œæ•´è·¯å¾‘ï¼š{full_folder_path}")
    
    # å°‹æ‰¾ä½¿ç”¨è€…
    print(f"\nğŸ” å°‹æ‰¾ä½¿ç”¨è€…ï¼š{folder_name}")
    users = sharer.find_user(folder_name)
    
    if not users:
        print("\nğŸ˜¢ æ‰¾ä¸åˆ°ç¬¦åˆçš„ä½¿ç”¨è€…")
        # å˜—è©¦æ‰‹å‹•è¼¸å…¥
        manual_email = input("\nğŸ’¡ è¦ä¸è¦æ‰‹å‹•è¼¸å…¥ä½¿ç”¨è€… emailï¼Ÿ(è¼¸å…¥ email æˆ–æŒ‰ Enter è·³é): ").strip()
        if manual_email:
            return sharer.share_folder_simple(full_folder_path, manual_email, 'Contribute')
        return False
    
    # å¦‚æœæœ‰å¤šå€‹ä½¿ç”¨è€…ï¼Œè®“ä½¿ç”¨è€…é¸æ“‡
    if len(users) > 1:
        print("\næ‰¾åˆ°å¤šå€‹ä½¿ç”¨è€…ï¼Œè«‹é¸æ“‡ï¼š")
        for idx, user in enumerate(users):
            print(f"  {idx+1}. {user['Title']} - {user['Email']}")
        
        choice = input("\nè«‹è¼¸å…¥ç·¨è™Ÿ (é è¨­é¸ 1): ").strip() or "1"
        try:
            selected_user = users[int(choice) - 1]
        except:
            selected_user = users[0]
    else:
        selected_user = users[0]
    
    print(f"\nå·²é¸æ“‡ï¼š{selected_user['Title']} ({selected_user['Email']})")
    
    # ç¢ºèªåˆ†äº«
    confirm = input("\nç¢ºå®šè¦åˆ†äº«å—ï¼Ÿ(Y/n): ").strip().lower()
    if confirm == 'n':
        print("å·²å–æ¶ˆåˆ†äº«")
        return False
    
    # åŸ·è¡Œåˆ†äº«
    return sharer.share_folder_simple(
        full_folder_path,
        selected_user['LoginName'] or selected_user['Email'],
        'Contribute'
    )

