            # æ¸¬è©¦é€£ç·š
            web = self.ctx.web
            self.ctx.load(web)
            self.ctx.execute_query()
            print(f"âœ… æˆåŠŸé€£æ¥åˆ°ï¼š{web.properties['Title']}")
