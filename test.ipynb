            print("   3. ç¢ºèªæœ‰æ¬Šé™å­˜å–æ­¤ç¶²ç«™")
            return False
    
    def find_user(self, display_name):
        """æ ¹æ“šé¡¯ç¤ºåç¨±å°‹æ‰¾ä½¿ç”¨è€…"""
        print(f"ğŸ” æœå°‹ä½¿ç”¨è€…ï¼š{display_name}")
        
        try:
            # æ–¹æ³• 1: ä½¿ç”¨ ensure_user ç›´æ¥å˜—è©¦
            try:
                # å…ˆå˜—è©¦ç”¨ email æ ¼å¼
                email_attempt = f"{display_name}@{urlparse(self.site_url).hostname.split('.')[0]}.com"
                user = self.ctx.web.ensure_user(email_attempt)
                self.ctx.execute_query()
                
                return [{
                    'Title': user.properties.get('Title'),
                    'Email': user.properties.get('Email'),
                    'LoginName': user.properties.get('LoginName'),
                    'Id': user.properties.get('Id')
                }]
            except:
                pass
            
            # æ–¹æ³• 2: æœå°‹æ‰€æœ‰ä½¿ç”¨è€…
            users = self.ctx.web.site_users
            self.ctx.load(users)
            self.ctx.execute_query()
            
            found_users = []
            for user in users:
                user_title = user.properties.get('Title', '')
                user_email = user.properties.get('Email', '')
                
                # æ’é™¤ç³»çµ±ä½¿ç”¨è€…
                if 'system' in user_title.lower() or not user_email:
                    continue
                
                # å®Œå…¨ç¬¦åˆæˆ–éƒ¨åˆ†ç¬¦åˆ
                if (display_name.lower() in user_title.lower() or 
                    display_name.lower() in user_email.lower()):
                    found_users.append({
                        'Title': user_title,
                        'Email': user_email,
                        'LoginName': user.properties.get('LoginName'),
                        'Id': user.properties.get('Id')
                    })
                    print(f"âœ… æ‰¾åˆ°ä½¿ç”¨è€…ï¼š{user_title} ({user_email})")
            
            if not found_users:
                print("âŒ æ‰¾ä¸åˆ°ä½¿ç”¨è€…")
                print("   å¯èƒ½çš„åŸå› ï¼š")
                print("   1. åå­—æ‹¼éŒ¯äº†")
                print("   2. ä½¿ç”¨è€…ä¸åœ¨é€™å€‹ç¶²ç«™")
                print("   3. ä½¿ç”¨è€…å»è²·é›æ’é‚„æ²’å›ä¾† ğŸ˜‚")
            
            return found_users
            
        except Exception as e:
            print(f"âŒ æœå°‹æ™‚ç™¼ç”ŸéŒ¯èª¤ï¼š{str(e)}")
            return []
    
